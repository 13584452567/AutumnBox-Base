<Application x:Class="AutumnBox.GUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="View/Windows/StartupWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Languages/zh-CN.xaml"/>
                <ResourceDictionary Source="/Resources/Themes/ThemeLight.xaml"/>
                <ResourceDictionary Source="/View/Custom/NiceWindowStyle.xaml"/>
                <ResourceDictionary Source="/Resources/Urls.xaml"/>
                <ResourceDictionary Source="/Resources/Converters.xaml"/>
                <ResourceDictionary Source="/Resources/ApiUrls.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
