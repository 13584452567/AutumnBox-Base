#/usr/bin/sh
#这个文件是用来快速制作AutumnBox的Release压缩包的



#delete
rm bin/*.log
rm bin/*.pdb
rm bin/*.config
rm bin/AutumnBox.application
rm bin/Tester.exe
rm bin/atbdata.sqlite
rm bin/*xml
rm -rf bin/app.publish
#make zip
mkdir make
mkdir make/AutumnBox
cp -r bin/* make/AutumnBox
cd make
zip -r ../out.zip AutumnBox
cd ..
rm -rf make
