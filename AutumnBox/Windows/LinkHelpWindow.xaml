<Window x:Class="AutumnBox.Windows.LinkHelpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutumnBox.UI"
        mc:Ignorable="d"
        Title="ContactWindow" Height="408.1" Width="565" ResizeMode="NoResize" WindowStyle="None" WindowStartupLocation="CenterOwner" Background="#FF009AD6" ShowInTaskbar="False">
    <Grid Margin="3,0,3,3" Background="White">
        <Label x:Name="labelTitle" Content="{DynamicResource LinkHelp}" HorizontalAlignment="Left" Width="566" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontFamily="Microsoft YaHei UI Light" FontSize="20" MouseMove="labelTitle_MouseMove" Margin="0,0,-1,0" Background="#FF009AD6" Height="53.2" VerticalAlignment="Top"/>
        <Label Content="{DynamicResource LinkQuestion1}" HorizontalAlignment="Left" Margin="10,58,0,0" VerticalAlignment="Top" Height="56" Width="356" VerticalContentAlignment="Center" FontFamily="Microsoft YaHei UI Light" FontSize="24" Background="#3365C5D4"/>
        <Image x:Name="imageClose" Height="30" Margin="527,10,10,0" VerticalAlignment="Top" Source="/AutumnBox.Res;component/Images/DynamicIcons/close_normal.png" MouseDown="imageClose_MouseDown" MouseEnter="imageClose_MouseEnter" MouseLeave="imageClose_MouseLeave"/>
        <TextBlock HorizontalAlignment="Left" Margin="153,119,0,0" TextWrapping="Wrap" Text="{DynamicResource LinkAnswer1}" VerticalAlignment="Top" Height="84" Width="396" FontSize="20" FontFamily="Microsoft YaHei UI Light" Background="#3365C5D4"/>
        <Label Content="{DynamicResource LinkQuestion3}" HorizontalAlignment="Left" Margin="10,304,0,0" VerticalAlignment="Top" Height="56" Width="143" VerticalContentAlignment="Center" FontFamily="Microsoft YaHei UI Light" FontSize="24" Background="#3365C5D4"/>
        <TextBlock HorizontalAlignment="Left" Margin="153,365,0,0" TextWrapping="Wrap" Text="{DynamicResource LinkAnswer3}" VerticalAlignment="Top" Height="31" Width="396" FontSize="20" FontFamily="Microsoft YaHei UI Light" Background="#3365C5D4" RenderTransformOrigin="0.485,1.645"/>
        <Label Content="{DynamicResource LinkQuestion1}" HorizontalAlignment="Left" Margin="10,208,0,0" VerticalAlignment="Top" Height="56" Width="356" VerticalContentAlignment="Center" FontFamily="Microsoft YaHei UI Light" FontSize="24" Background="#3365C5D4"/>
        <TextBlock HorizontalAlignment="Left" Margin="153,269,0,0" TextWrapping="Wrap" Text="{DynamicResource LinkAnswer2}" VerticalAlignment="Top" Height="30" Width="396" FontSize="20" FontFamily="Microsoft YaHei UI Light" Background="#3365C5D4"/>
    </Grid>
</Window>
