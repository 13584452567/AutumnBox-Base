<Window x:Class="AutumnBox.AboutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutumnBox"
        mc:Ignorable="d"
        Title="AboutWindow" Height="470.706" Width="708.471" WindowStyle="None">
    <Grid>
        <Label x:Name="labelTitle" Content="{DynamicResource AboutThisSoftware}" HorizontalAlignment="Left" Width="700" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontFamily="Microsoft YaHei UI Light" FontSize="20" Margin="0,0,0,410" Background="#FF009AD6" Height="53.2" MouseMove="labelTitle_MouseMove"/>
        <Label x:Name="labelClose" Content="{DynamicResource Close}" HorizontalAlignment="Left" Width="700" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontFamily="Microsoft YaHei UI Light" FontSize="20" Margin="0,410,0,0" Background="#FF009AD6" Height="53.2" MouseDown="labelClose_MouseDown"/>
        <Label Content="Label" HorizontalAlignment="Left" Margin="10,58,0,0" VerticalAlignment="Top" Height="347" Width="680" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontFamily="Microsoft YaHei UI Light" FontSize="20"/>

    </Grid>
</Window>
