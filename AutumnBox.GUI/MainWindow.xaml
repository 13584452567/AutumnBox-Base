<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutumnBox.GUI"
        xmlns:cstm="clr-namespace:AutumnBox.GUI.UI.Cstm"
        xmlns:grids="clr-namespace:AutumnBox.GUI.UI.Grids"
        x:Name="_MainWindow" x:Class="AutumnBox.GUI.MainWindow"
        mc:Ignorable="d"
        Background="{DynamicResource BackBrush}" Title="AutumnBox" Height="650" Width="1155" Foreground="#FF1B1919" ResizeMode="NoResize" Loaded="MainWindow_Loaded" WindowStyle="None" AllowsTransparency="True">
    <Grid Name="MainGrid">
        <Grid Name="GridToolsMain" Background="#00E5E5E5" Margin="0,45,0,0">
            <grids:RebootButtonsGrid x:Name="RebootGrid" Margin="0,12,29,0" HorizontalAlignment="Right" Width="265" VerticalAlignment="Top" Height="126" BorderBrush="{DynamicResource BorderBrush}"/>
            <grids:DeviceInfoPanel x:Name="DevInfoPanel" Margin="10,0,0,0" Height="222" VerticalAlignment="Top" HorizontalAlignment="Left" Width="650"/>
            <Button Foreground="{DynamicResource ForgBrush}" x:Name="buttonStartShell"  Content="{DynamicResource btnADBCMD}" Margin="861,142,29,0" VerticalAlignment="Top" Height="80" Background="{x:Null}" BorderBrush="{DynamicResource BorderBrush}" FontSize="14" FontFamily="Microsoft YaHei UI Light" Click="ButtonStartShell_Click" />
            <TabControl Name="TabFunctions" Background="{x:Null}" BorderBrush="{DynamicResource BorderBrush}" Margin="10,227,221,10">
                <TabItem TabIndex="1" Header="{DynamicResource tbiOnPowerOn}" BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" Margin="-2,0,0,0" HorizontalAlignment="Left" Width="200" Background="{DynamicResource BasicBrush}" Foreground="{DynamicResource ForgBrushOnBasic}" FontFamily="Microsoft YaHei UI Light" FontSize="16">
                    <grids:PoweronFunctions x:Name="PoweronFuncs" Margin="-8,0,4,0"></grids:PoweronFunctions>
                </TabItem>
                <TabItem TabIndex="2" Header="{DynamicResource tbiOnRecovery}" BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" Margin="-2,0,0,0" HorizontalAlignment="Left" Width="200" Background="{DynamicResource BasicBrush}" Foreground="{DynamicResource ForgBrushOnBasic}" FontFamily="Microsoft YaHei UI Light" FontSize="16">
                    <grids:RecoveryFunctions x:Name="RecoveryFuncs"/>
                </TabItem>
                <TabItem  TabIndex="3" Header="{DynamicResource tbiOnFastboot}" BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" Margin="-2,0,0,0" HorizontalAlignment="Left" Width="200" Background="{DynamicResource BasicBrush}" Foreground="{DynamicResource ForgBrushOnBasic}" FontFamily="Microsoft YaHei UI Light" FontSize="16">
                    <grids:FastbootFunctions x:Name="FastbootFuncs"></grids:FastbootFunctions>
                </TabItem>
            </TabControl>
            <Label Background="{DynamicResource BasicBrush}" Content="" Margin="0,12,299,0" HorizontalAlignment="Right" Width="1" Height="213" VerticalAlignment="Top"/>
            <TextBox Foreground="{DynamicResource ForgBrush}" x:Name="textBoxGG" Height="88" Margin="939,255,10,0" TextWrapping="Wrap" Text="..." VerticalAlignment="Top" IsReadOnly="True" Background="{x:Null}" FontSize="14"/>
            <Button Foreground="{DynamicResource ForgBrush}" x:Name="BtnHelp" Content="{DynamicResource btnHelp}" Margin="0,348,10,0" VerticalAlignment="Top" Height="38" Background="{x:Null}" BorderBrush="{DynamicResource BorderBrush}" FontSize="14" FontFamily="Microsoft YaHei UI Light" Click="BtnHelp_Click" HorizontalAlignment="Right" Width="206" />
            <ListBox x:Name="DevicesListBox" DisplayMemberPath="Id" Height="191" Margin="0,31,311,0" VerticalAlignment="Top" SelectionChanged="DevicesListBox_SelectionChanged" Background="#00000000" Foreground="{DynamicResource BasicBrush}" BorderBrush="#FFABADB3" HorizontalAlignment="Right" Width="179"/>
            <Button x:Name="buttonLinkHelp"  Foreground="{DynamicResource ForgBrush}" Content="{DynamicResource LinkHelp}" Margin="0,390,38,0" VerticalAlignment="Top" Height="36" Background="{x:Null}" BorderBrush="{DynamicResource BorderBrush}" FontSize="14" FontFamily="Microsoft YaHei UI Light" Click="ButtonLinkHelp_Click" Width="178" HorizontalAlignment="Right"/>
            <Label Content="{DynamicResource Devices}" Foreground="{DynamicResource ForgBrush}" Margin="0,0,311,0" VerticalAlignment="Top" HorizontalContentAlignment="Center" HorizontalAlignment="Right" Width="179"/>
        </Grid>
        <cstm:CstmTitleBar x:Name="TitleBar" Height="24" VerticalAlignment="Top"/>
        <!--<TabControl Margin="0,40,0,0" TabStripPlacement="Left" BorderBrush="{DynamicResource BackBrush}" Background="{x:Null}">
            <TabItem Foreground="{DynamicResource ForgBrushOnBasic}" Header="{DynamicResource tbiTools}" Background="{DynamicResource BasicBrush}" Margin="-2,-2,0,-10" BorderBrush="{DynamicResource BasicBrush}" MinHeight="130" FontFamily="Microsoft YaHei UI Light" FontSize="10">
 
            </TabItem>
            <TabItem Foreground="{DynamicResource ForgBrushOnBasic}" Header="{DynamicResource tbiBookMarker}" Background="{DynamicResource BasicBrush}" Margin="-2,-3,0,3" BorderBrush="{DynamicResource BasicBrush}" MinHeight="130" FontFamily="Microsoft YaHei UI Light" Height="130" FontSize="10">
                <Grid Background="#00E5E5E5" Margin="0,0,-3,7">
                    <TabControl Margin="10,10,10,0" Background="Transparent" BorderBrush="{DynamicResource BasicBrush}">
                        <TabItem  Header="{DynamicResource tbiDownloads}" BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" Margin="-2,0,2,0" HorizontalAlignment="Right" Width="200" Background="#7F009AD6" Foreground="{DynamicResource ForgBrush}" FontSize="16" FontFamily="Microsoft YaHei UI Light">
                            <cstm:BookMarksControl Margin="10,0,1,-2"/>
                        </TabItem>
                    </TabControl>
                </Grid>
            </TabItem>
