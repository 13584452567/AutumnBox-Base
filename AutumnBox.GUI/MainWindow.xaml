<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutumnBox.GUI"
        xmlns:cstm="clr-namespace:AutumnBox.GUI.UI.Cstm"
        xmlns:cpnel="clr-namespace:AutumnBox.GUI.UI.FuncPanels"
        x:Name="_MainWindow" x:Class="AutumnBox.GUI.MainWindow"
        mc:Ignorable="d"
        Background="{DynamicResource BackBrush}" Title="AutumnBox" Height="682.667" Width="1155" Foreground="#FF1B1919" ResizeMode="NoResize" Loaded="MainWindow_Loaded" AllowsTransparency="True" WindowStyle="None">
    <Grid Name="GridBase" Margin="3,0,3,3">
        <cstm:CstmTitleBar x:Name="TitleBar" Height="35" VerticalAlignment="Top"/>
        <Label Content="Label" HorizontalAlignment="Left" Margin="-1,41,0,0" VerticalAlignment="Top" Height="1" Background="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Foreground="{x:Null}" Width="1149"/>
        <Grid Name="GridMain" Background="#00E5E5E5" Margin="0,42,0,-13" Height="651" VerticalAlignment="Top">
            <cpnel:RebootBtnPanel x:Name="RebootGrid" Margin="0,26,23,0" HorizontalAlignment="Right" Width="265" VerticalAlignment="Top" Height="126" BorderBrush="{DynamicResource BorderBrush}"/>
            <cpnel:DeviceInfoPanel x:Name="DevInfoPanel" Margin="10,0,0,0" Height="222" VerticalAlignment="Top" HorizontalAlignment="Left" Width="650"/>
            <Button Foreground="{DynamicResource {x:Static SystemColors.ScrollBarBrushKey}}" x:Name="buttonStartShell"  Content="{DynamicResource btnADBCMD}" Margin="861,157,23,0" VerticalAlignment="Top" Height="54" Background="{x:Null}" BorderBrush="{DynamicResource BorderBrush}" FontSize="14" FontFamily="Microsoft YaHei UI Light" Click="ButtonStartShell_Click" />
            <Button Foreground="{DynamicResource ForgBrush}" x:Name="BtnSettings"  Content="{DynamicResource btnSetting}" Margin="940,590,6,0" VerticalAlignment="Top" Height="35" Background="{x:Null}" BorderBrush="{DynamicResource BorderBrush}" FontSize="14" FontFamily="Microsoft YaHei UI Light" Click="BtnSettings_Click" />
            <Button Foreground="{DynamicResource ForgBrush}" x:Name="BtnAbout"  Content="{DynamicResource btnAbout}" Margin="939,550,6,0" VerticalAlignment="Top" Height="35" Background="{x:Null}" BorderBrush="{DynamicResource BorderBrush}" FontSize="14" FontFamily="Microsoft YaHei UI Light" Click="BtnAbout_Click" />
            <Button Foreground="{DynamicResource ForgBrush}" x:Name="BtnDonate"  Content="{DynamicResource Donate}" Margin="940,510,6,0" VerticalAlignment="Top" Height="35" Background="{x:Null}" BorderBrush="{DynamicResource BorderBrush}" FontSize="14" FontFamily="Microsoft YaHei UI Light" Click="BtnDonate_Click" />
            <Button Foreground="{DynamicResource ForgBrush}" x:Name="BtnLinkHelp"  Content="{DynamicResource LinkHelp}" Margin="939,468,6,0" VerticalAlignment="Top" Height="37" Background="{x:Null}" BorderBrush="{DynamicResource BorderBrush}" FontSize="14" FontFamily="Microsoft YaHei UI Light" Click="BtnLinkHelp_Click" />
            <Image Source="Resources/Images/Icons/donate.png" HorizontalAlignment="Left" Height="27" Margin="963,514,0,0" VerticalAlignment="Top" Width="27"/>
            <TextBox Foreground="{DynamicResource ForgBrush}" x:Name="textBoxGG" Height="88" Margin="940,285,6,0" TextWrapping="Wrap" Text="..." VerticalAlignment="Top" IsReadOnly="True" Background="{x:Null}" FontSize="14"/>
            <cpnel:DevicesPanel  x:Name="DevicesPanel" Margin="667,12,293,406"></cpnel:DevicesPanel>
            <TextBlock x:Name="TBHelp" HorizontalAlignment="Left" Margin="1046,221,0,0" TextWrapping="Wrap" Text="{DynamicResource btnHelp}" VerticalAlignment="Top" TextDecorations="Underline" Foreground="{DynamicResource ForgBrush}" FontSize="18" MouseDown="TBHelp_MouseDown" Height="24" Width="80"/>
            <TabControl HorizontalAlignment="Left" Height="370" Margin="10,255,0,0" VerticalAlignment="Top" Width="924" Background="#20000000" BorderBrush="{DynamicResource BorderBrush}">
                <TabItem TabIndex="0" Header="{DynamicResource tbiOnPowerOn}" Margin="-2,-2,-2,0" Foreground="{DynamicResource ForgBrush}" FontSize="16">
                    <cpnel:PoweronFuncPanel x:Name="PoweronFuncs" Margin="0,0,-2,0"></cpnel:PoweronFuncPanel>
                </TabItem>
                <TabItem TabIndex="1"  Header="{DynamicResource tbiOnRecovery}" Margin="1,-2,-5,0" Foreground="{DynamicResource ForgBrush}" FontSize="16">
                    <cpnel:RecFuncPanel x:Name="RecoveryFuncs"/>
                </TabItem>
                <TabItem TabIndex="2"  Header="{DynamicResource tbiOnFastboot}" Margin="5,-2,-9,0" Foreground="{DynamicResource ForgBrush}" FontSize="16">
                    <cpnel:FastbootFuncPanel x:Name="FastbootFuncs"></cpnel:FastbootFuncPanel>
                </TabItem>
            </TabControl>
        </Grid>

    </Grid>
</Window>
