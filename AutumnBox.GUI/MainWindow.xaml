                <Grid Background="#00E5E5E5" Margin="0,0,-3,7">
                    <TabControl Margin="10,10,10,0" Background="Transparent" BorderBrush="{DynamicResource BasicBrush}">
                        <TabItem Header="{DynamicResource tbiAbout}" BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" Margin="0,0,0,0" HorizontalAlignment="Right" Width="200" Background="#7F009AD6" Foreground="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" FontSize="16" FontFamily="Microsoft YaHei UI Light">
                            <grids:AboutControl x:Name="AboutControl"/>
                        </TabItem>
                        <TabItem Header="{DynamicResource tbiSettings}" BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" Margin="0,0,0,0" HorizontalAlignment="Right" Width="200" Background="#7F009AD6" Foreground="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" FontSize="16" FontFamily="Microsoft YaHei UI Light">
                            <grids:Settings x:Name="Settings" Margin="0,0,-1,0"></grids:Settings>
                        </TabItem>
                    </TabControl>
                </Grid>
            </TabItem>
        </TabControl>-->
    </Grid>
</Window>
