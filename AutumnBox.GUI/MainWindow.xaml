<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutumnBox.GUI"
        xmlns:cstm="clr-namespace:AutumnBox.GUI.UI.Cstm"
        xmlns:cpnel="clr-namespace:AutumnBox.GUI.UI.FuncPanels"
        x:Name="_MainWindow" x:Class="AutumnBox.GUI.MainWindow"
        mc:Ignorable="d"
    Closed="_MainWindow_Closed"
        Background="{DynamicResource BroderBrushKey}" Title="AutumnBox" Height="682.667" Width="1155" Foreground="#FF1B1919" Loaded="MainWindow_Loaded"  WindowStyle="None"  WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    
    <Grid>
        <Grid  Background="{DynamicResource MainBackgroundBrushKey}" Name="GridBase" Margin="1,0,1,1">
            <WebBrowser x:Name="WTF" Width="1" Height="0" Margin="963,271,185,408"/>
            <cstm:CstmTitleBar  Background="{DynamicResource MainTitleBarBackgroundBrushKey}" x:Name="TitleBar" Height="40" VerticalAlignment="Top"/>

            <Grid Name="GridMain"  Margin="0,40,0,-13" Height="653" VerticalAlignment="Top">

                <cpnel:RebootBtnPanel x:Name="RebootGrid" Margin="0,10,23,0" HorizontalAlignment="Right" Width="265" VerticalAlignment="Top" Height="126" />
                <cpnel:DeviceInfoPanel x:Name="DevInfoPanel" Margin="10,10,0,0" Height="212" VerticalAlignment="Top" HorizontalAlignment="Left" Width="650"/>
                <cpnel:DevicesPanel  x:Name="DevicesPanel" Margin="667,0,293,418"/>

                <Button Style="{DynamicResource AtmbxButtonStyle}"  x:Name="buttonStartShell"  Content="{DynamicResource btnADBCMD}" Margin="861,141,23,0" VerticalAlignment="Top" Height="54"  FontSize="14" FontFamily="Microsoft YaHei UI Light" Click="ButtonStartShell_Click" />
                <Button  Style="{DynamicResource AtmbxButtonStyle}" x:Name="BtnSettings"  Content="{DynamicResource btnSetting}" Margin="940,590,9,0" VerticalAlignment="Top" Height="35"   FontSize="14"  Click="BtnSettings_Click" />
                <Button  Style="{DynamicResource AtmbxButtonStyle}" x:Name="BtnAbout"  Content="{DynamicResource btnAbout}" Margin="939,550,9,0" VerticalAlignment="Top" Height="35"  FontSize="14"  Click="BtnAbout_Click" />
                <Button  Style="{DynamicResource AtmbxButtonStyle}" x:Name="BtnDonate"  Content="{DynamicResource Donate}" Margin="940,510,9,0" VerticalAlignment="Top" Height="35"   FontSize="14"  Click="BtnDonate_Click" />
                <Image Source="Resources/Images/Icons/donate.png" HorizontalAlignment="Left" Height="27" Margin="943,514,0,0" VerticalAlignment="Top" Width="27"/>
                <TextBox BorderBrush="{DynamicResource BroderBrushKey}" Foreground="{DynamicResource ForegroundBrushKey}" x:Name="textBoxGG" Height="93" Margin="940,260,9,0" TextWrapping="Wrap" Text="..." VerticalAlignment="Top" IsReadOnly="True" Background="{x:Null}" FontSize="14"/>


                <cstm:PotdBox Margin="940,355,10,146" Height="150" Width="200"/>
                <TabControl SelectionChanged="TBCFuncs_SelectionChanged" x:Name="TBCFuncs" HorizontalAlignment="Left" Height="392" Margin="10,233,0,0" VerticalAlignment="Top" Width="924" Background="{x:Null}" BorderBrush="{DynamicResource BroderBrushKey}">
                    <TabItem  Style="{DynamicResource AtmbxTabItem}" TabIndex="0" Header="{DynamicResource tbiStart}" Margin="-2,-2,-14,0"  FontSize="16">
                        <cpnel:StartPanel x:Name="StartPanel" Margin="0,0,0,0"></cpnel:StartPanel>
                    </TabItem>
                    <TabItem  Style="{DynamicResource AtmbxTabItem}" TabIndex="1" Header="{DynamicResource tbiOnPowerOn}" Margin="14,-2,-18,0"  FontSize="16">
                        <ScrollViewer  x:Name="PoweronFuncsViewer" Grid.Column="1"  HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Visible" Template="{DynamicResource AtmbxScrollViewer}">
                            <cpnel:PoweronFuncPanel x:Name="PoweronFuncs" Margin="0,0,0,0"/>
                        </ScrollViewer>
                    </TabItem>
                    <TabItem Style="{DynamicResource AtmbxTabItem}" TabIndex="2"  Header="{DynamicResource tbiOnRecovery}" Margin="18,-2,-23,0"  FontSize="16">
                        <cpnel:RecFuncPanel Margin="0,0,0,0" x:Name="RecoveryFuncs"/>
                    </TabItem>
                    <TabItem Style="{DynamicResource AtmbxTabItem}" TabIndex="3"  Header="{DynamicResource tbiOnFastboot}" Margin="23,-2,-26,0"  FontSize="16">
                        <cpnel:FastbootFuncPanel x:Name="FastbootFuncs"/>
                    </TabItem>
                    <TabItem Style="{DynamicResource AtmbxTabItem}" TabIndex="4"  Header="{DynamicResource tbiExtension}" Margin="26,-2,-29,0"  FontSize="16">
                        <cpnel:ThridPartyFunctionPanel Margin="0,0,0,0" x:Name="ThridPartyFuncs"/>
                    </TabItem>
                </TabControl>
                
            </Grid>
        </Grid>
        <!--<cstm:LoadingPanel x:Name="LoadingPanel"></cstm:LoadingPanel>-->
    </Grid>
</Window>
