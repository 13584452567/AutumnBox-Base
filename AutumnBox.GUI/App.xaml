<Application x:Class="AutumnBox.GUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AutumnBox.GUI"
             StartupUri="StartWindow/StartWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/AutumnBox.GUI.Resources;component/Lang/zh-CN.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/AutumnBox.GUI.Resources;component/Theme/BasicTheme.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/AutumnBox.GUI.Resources;component/Other/links.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
