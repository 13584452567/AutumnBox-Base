<Application x:Class="AutumnBox.GUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AutumnBox.GUI"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Langs/zh-CN.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/ColorThemes/BasicTheme.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/links.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
