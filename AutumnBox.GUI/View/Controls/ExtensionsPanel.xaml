<UserControl
    x:Class="AutumnBox.GUI.View.Controls.ExtensionsPanel"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:AutumnBox.GUI.View.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignWidth="400" x:Name="mThis">

    <StackPanel Margin="20,10,20,10">
        <TextBlock  Foreground="Black" FontSize="25" Text="{Binding Title,ElementName=mThis}"/>
        <WrapPanel x:Name="docksContainer"/>
    </StackPanel>
</UserControl>
