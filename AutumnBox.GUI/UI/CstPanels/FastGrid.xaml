<UserControl x:Class="AutumnBox.GUI.UI.CstPanels.FastGrid"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AutumnBox.GUI.UI.CstPanels"
             mc:Ignorable="d" Width="579.5" Height="358" Background="{DynamicResource BackBrush}" Loaded="UserControl_Loaded">
    <Grid x:Name="GridContent">
        <Button Content="{DynamicResource btnClose}" VerticalAlignment="Top" Background="{x:Null}" Foreground="{DynamicResource ForgBrush}" Height="34" FontFamily="Microsoft YaHei UI Light" Click="Button_Click" HorizontalAlignment="Right" Width="108"/>
    </Grid>
</UserControl>
