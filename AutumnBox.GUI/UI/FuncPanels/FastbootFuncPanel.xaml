             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AutumnBox.GUI.UI.FuncPanels"
             mc:Ignorable="d" 
             d:DesignHeight="336" d:DesignWidth="922">
    <Grid x:Name="MainGrid">
        <Button Style="{DynamicResource AtmbxButtonStyle}" x:Name="buttonFlashCustomRecovery" Content="{DynamicResource btnFlashCustomRecoveryImg}" Margin="10,10,10,167" FontSize="18"  Click="ButtonFlashCustomRecovery_Click" />
