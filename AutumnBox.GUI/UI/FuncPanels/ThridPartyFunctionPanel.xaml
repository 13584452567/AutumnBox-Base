<UserControl x:Class="AutumnBox.GUI.UI.FuncPanels.ThridPartyFunctionPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AutumnBox.GUI.UI.FuncPanels"
             mc:Ignorable="d" 
             d:DesignHeight="366" d:DesignWidth="922">
    <Grid>
        <DataGrid x:Name="GridExtendModule" IsReadOnly="True" HorizontalAlignment="Left" Height="306" Margin="10,50,0,0" VerticalAlignment="Top" Width="902">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding Name  }"  ClipboardContentBinding="{Binding Name}" Width="100" Header="模块名"/>
                <DataGridTextColumn Binding="{Binding Version}"  Width="70" Header="版本"/>
                <DataGridTextColumn Binding="{Binding Auth}"  Width="70" Header="开发者"/>
                <DataGridTextColumn Binding="{Binding ContactMail}"  Width="100" Header="联系方式"/>
                <DataGridTextColumn Binding="{Binding Desc}"  Width="500" Header="说明"/>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</UserControl>
