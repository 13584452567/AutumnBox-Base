<UserControl x:Class="AutumnBox.GUI.UI.FuncPanels.PoweronFuncPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AutumnBox.GUI.UI.FuncPanels"
             mc:Ignorable="d" d:DesignWidth="922" Width="922" Height="336">
    <Grid x:Name="MainGrid">
        <Label Content="NO ROOT" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="36" Width="190" Foreground="{DynamicResource ForgBrush}"/>
        <Button  x:Name="buttonPushFileToSdcardPoweron"   Content="{DynamicResource btnPushFileToSdcard}" Margin="10,71,0,0" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" Click="ButtonPushFileToSdcard_Click" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" Height="40" VerticalAlignment="Top" HorizontalAlignment="Left" Width="380"/>
        <Button  x:Name="ButtonInstallApk"   Content="{DynamicResource btnInstallApk}" Margin="395,71,0,0" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" Click="ButtonInstallApk_Click" HorizontalAlignment="Left" Width="215" Height="40" VerticalAlignment="Top"/>
        <Button  x:Name="ButtonVirtualBtnHide"   Content="{DynamicResource btnVBHide}" Margin="201,116,0,0" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" Click="ButtonVirtualBtnHide_Click" Height="40" VerticalAlignment="Top" HorizontalAlignment="Left" Width="189"/>
        <Button  x:Name="ButtonScreentShot"   Content="{DynamicResource btnScreenShot}" Margin="395,116,0,0" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" Click="ButtonScreentShot_Click" HorizontalAlignment="Left" Width="215" Height="40" VerticalAlignment="Top"/>
        <Button x:Name="ButtonChangeDpi" Content="{DynamicResource btnDpiChange}" Margin="10,116,0,0" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" Click="ButtonChangeDpi_Click" HorizontalAlignment="Left" Width="186" Height="40" VerticalAlignment="Top"/>
        <Button x:Name="BtnBackupDcim" Content="{DynamicResource btnBackupDcim}" Margin="10,162,0,0" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="186" Height="40" VerticalAlignment="Top" Click="BtnBackupDcim_Click"/>
        <Button x:Name="BtnAppManager" Content="{DynamicResource btnAppManager}" Margin="201,162,0,0" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="409" Height="40" VerticalAlignment="Top" Click="BtnAppManager_Click" IsEnabled="False" Visibility="Hidden"/>

        <Label Background="{DynamicResource BorderBrush}" Content="" Margin="0,23,295,23" HorizontalAlignment="Right" Width="1" Foreground="{x:Null}"/>
        <Label Content="ROOT" HorizontalAlignment="Left" Margin="632,10,0,0" VerticalAlignment="Top" FontSize="36" Width="110" Height="59" Foreground="{DynamicResource ForgBrush}"/>
        <Button x:Name="ButtonExtractRecImg"   Content="{DynamicResource btnExtractRecImg}" Margin="632,91,0,0" VerticalAlignment="Top" Height="40" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="141" Click="ButtonExtractRecImg_Click" />
        <Button x:Name="ButtonExtractBootImg"   Content="{DynamicResource btnExtractBootImg}" Margin="632,171,0,0" VerticalAlignment="Top" Height="40" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="141" Click="ButtonExtractBootImg_Click" />
        <Button x:Name="ButtonFlashBootImg"   Content="{DynamicResource btnFlashBootImg}" Margin="778,171,0,0" VerticalAlignment="Top" Height="40" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="134" Click="ButtonFlashBootImg_Click" />
        <Button x:Name="ButtonFlashRecImg"   Content="{DynamicResource btnFlashRecImg}" Margin="778,91,0,0" VerticalAlignment="Top" Height="40" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="134" Click="ButtonFlashRecImg_Click"/>
        <Button x:Name="ButtonDeleteScreenLock"   Content="{DynamicResource btnDeleteScreenLock}" Margin="632,281,0,0" VerticalAlignment="Top" Height="45" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="280" Click="ButtonDeleteScreenLock_Click"/>
        <Button x:Name="buttonUnlockMiSystem"  Content="{DynamicResource btnUnlockSystem}" Margin="632,234,0,0" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" Click="ButtonUnlockMiSystem_Click" Height="42" VerticalAlignment="Top" HorizontalAlignment="Left" Width="280"/>

        <Label Content="{DynamicResource Bridge}" Margin="10,198,0,0" VerticalAlignment="Top" FontSize="18" Background="{x:Null}" Foreground="{DynamicResource ForgBrush}" HorizontalAlignment="Left" Width="611" Height="31"/>
        <Button x:Name="buttonStartBrventService" Content="{DynamicResource btnStartBrventService}" Margin="10,234,0,0" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" Click="ButtonStartBrventService_Click" Height="42" VerticalAlignment="Top" HorizontalAlignment="Left" Width="352"/>
        <Button x:Name="ButtonIceBoxAct"   Content="{DynamicResource btnIceBoxAct}" Margin="367,234,0,0" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="254" Click="ButtonIceBoxAct_Click" Height="42" VerticalAlignment="Top"/>
        <Button x:Name="ButtonIslandAct"   Content="{DynamicResource btnIslandAct}" Margin="459,281,0,0" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="162" Click="ButtonIslandAct_Click" Height="45" VerticalAlignment="Top"/>
        <Button x:Name="ButtonAirForzenAct"   Content="{DynamicResource btnAirForzenAct}" Margin="297,281,0,0" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="157" Click="ButtonAirForzenAct_Click" Height="45" VerticalAlignment="Top"/>
        <Button x:Name="ButtonShizukuManager"   Content="{DynamicResource btnShizukuManagerAct}" Margin="10,281,0,0" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="282" Click="ButtonShizukuManager_Click" Height="45" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
