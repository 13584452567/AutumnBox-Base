<UserControl x:Class="AutumnBox.GUI.UI.Grids.RecFuncPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AutumnBox.GUI.UI.Grids"
             mc:Ignorable="d" Height="336" Width="922">
    <Grid x:Name="MainGrid">
        <Button  x:Name="buttonPushFileToSdcard"   Content="{DynamicResource btnPushFileToSdcard}" Margin="10,10,20,0" Background="{x:Null}" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" Click="ButtonPushFileToSdcard_Click" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{StaticResource ForgBrush}" RenderTransformOrigin="0.494,0.085" Height="151" VerticalAlignment="Top"/>
        <Button x:Name="ButtonSideload" Content="{DynamicResource btnSideloadFlash}" Margin="10,0,20,10" Background="#00E6E1E1" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" Foreground="{StaticResource ForgBrush}" Click="ButtonSideload_Click" RenderTransformOrigin="0.697,1.991" Height="151" VerticalAlignment="Bottom"/>
    </Grid>
</UserControl>
