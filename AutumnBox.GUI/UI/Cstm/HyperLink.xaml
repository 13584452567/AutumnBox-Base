<UserControl x:Class="AutumnBox.GUI.UI.Cstm.HyperLink"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             x:Name="mUserControl"
             xmlns:local="clr-namespace:AutumnBox.GUI.UI.Cstm"
             FontSize="23"
             Foreground="{DynamicResource BackgroundBrushKey}"
             mc:Ignorable="d" Height="28" Width="106" MouseEnter="UserControl_MouseEnter" MouseLeave="UserControl_MouseLeave">
    <TextBlock  x:Name="TBMain" TextAlignment="Center" Style="{DynamicResource AtmbxTextBlockStyle}"  MouseDown="TBMain_MouseDown" Text="{Binding Path=Text,ElementName=mUserControl}"/>
</UserControl>
