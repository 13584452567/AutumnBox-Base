<UserControl x:Class="AutumnBox.GUI.UI.Fp.FastPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AutumnBox.GUI.UI.Fp"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Background="{DynamicResource BackgroundBurshKey}">
    <Grid>
        <Grid x:Name="Container"/>
        <Button Style="{DynamicResource AtmbxButtonStyle}" x:Name="BtnClose"  Content="{DynamicResource btnClose}" VerticalAlignment="Top" Click="BtnClose_Click" HorizontalAlignment="Right" Width="75"/>
    </Grid>
</UserControl>
