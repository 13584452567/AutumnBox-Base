<Grid x:Class="AutumnBox.GUI.UI.Grids.RecoveryFunctions"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AutumnBox.GUI.UI.Grids"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="998">
    <Button  x:Name="buttonPushFileToSdcard"   Content="{DynamicResource btnPushFileToSdcard}" Margin="10,10,0,0" VerticalAlignment="Top" Height="135" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" Click="ButtonPushFileToSdcard_Click" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{StaticResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="968"/>
    <Button x:Name="ButtonSideload" Content="{DynamicResource btnSideloadFlash}" HorizontalAlignment="Left" Margin="10,150,0,0" VerticalAlignment="Top" Width="968" Height="140" Background="#00E6E1E1" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" Foreground="{StaticResource ForgBrush}" Click="ButtonSideload_Click" RenderTransformOrigin="0.697,1.991"/>
</Grid>
