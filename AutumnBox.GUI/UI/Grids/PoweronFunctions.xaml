<Grid x:Class="AutumnBox.GUI.UI.Grids.PoweronFunctions"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AutumnBox.GUI.UI.Grids"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="988" Background="White">
    <Button  x:Name="buttonPushFileToSdcardPoweron"   Content="{DynamicResource btnPushFileToSdcard}" Margin="10,131,0,0" VerticalAlignment="Top" Height="48" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" Click="ButtonPushFileToSdcard_Click" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="492"/>
    <Button  x:Name="ButtonInstallApk"   Content="{DynamicResource btnInstallApk}" Margin="507,130,0,0" VerticalAlignment="Top" Height="49" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="166" Click="ButtonInstallApk_Click"/>
    <Button  x:Name="ButtonScreentShot"   Content="{DynamicResource btnScreenShot}" Margin="507,79,0,0" VerticalAlignment="Top" Height="47" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="166" Click="ButtonScreentShot_Click"/>
    <Button x:Name="buttonUnlockMiSystem"  Content="{DynamicResource btnUnlockSystem}" Margin="686,148,0,0" VerticalAlignment="Top" Height="31" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" Click="ButtonUnlockMiSystem_Click" HorizontalAlignment="Left" Width="292"/>
    <Button x:Name="buttonStartBrventService" Content="{DynamicResource btnStartBrventService}" Margin="10,79,0,0" VerticalAlignment="Top" Height="47" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="492" Click="ButtonStartBrventService_Click"/>
    <Button x:Name="ButtonChangeDpi" Content="{DynamicResource btnDpiChange}" Margin="10,184,0,0" VerticalAlignment="Top" Height="49" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="166" Click="ButtonChangeDpi_Click"/>
    <Button x:Name="ButtonFullBackup"   Content="{DynamicResource btnAndroidFullBackup}" Margin="181,184,0,0" VerticalAlignment="Top" Height="48" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="148" Click="ButtonFullBackup_Click"/>
    <Label Background="{DynamicResource BasicBrush}" Content="" Margin="678,26,0,24" HorizontalAlignment="Left" Width="1"/>
    <Button x:Name="ButtonExtractRecImg"   Content="{DynamicResource btnExtractRecImg}" Margin="686,113,0,0" VerticalAlignment="Top" Height="30" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="140" />
    <Button x:Name="ButtonExtractBootImg"   Content="{DynamicResource btnExtractBootImg}" Margin="686,79,0,0" VerticalAlignment="Top" Height="30" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="140" />
    <Button x:Name="ButtonFlashBootImg"   Content="{DynamicResource btnFlashBootImg}" Margin="838,113,0,0" VerticalAlignment="Top" Height="30" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="140" />
    <Button x:Name="ButtonFlashRecImg"   Content="{DynamicResource btnFlashRecImg}" Margin="838,79,0,0" VerticalAlignment="Top" Height="30" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="140"/>
    <Button x:Name="ButtonFullBackup_Copy"   Content="{DynamicResource btnTapEmu}" Margin="334,185,0,0" VerticalAlignment="Top" Height="48" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="168"/>
    <Button x:Name="ButtonDeleteScreenLock"   Content="{DynamicResource btnDeleteScreenLock}" Margin="686,185,0,0" VerticalAlignment="Top" Height="29" Background="#007C7272" BorderBrush="{DynamicResource BorderBrush}" FontSize="18" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="{DynamicResource ForgBrush}" RenderTransformOrigin="0.494,0.085" HorizontalAlignment="Left" Width="292"/>
    <Label Content="ROOT" HorizontalAlignment="Left" Margin="686,16,0,0" VerticalAlignment="Top" FontSize="36" Width="122"/>
    <Label Content="NO ROOT" HorizontalAlignment="Left" Margin="10,16,0,0" VerticalAlignment="Top" FontSize="36" Width="190"/>

</Grid>
